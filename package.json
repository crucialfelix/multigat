{
  "name": "multigat",
  "description": "Build gatsby sites en masse",
  "version": "0.0.0",
  "author": "Chris Sattinger @crucialfelix",
  "bin": {
    "multigat": "./bin/run"
  },
  "bugs": "https://github.com/crucialfelix/multigat/issues",
  "dependencies": {
    "@oclif/command": "1.4.33",
    "@oclif/config": "1.6.32",
    "@oclif/plugin-help": "2.0.5",
    "@types/chokidar": "1.7.5",
    "@types/del": "3.0.1",
    "@types/es6-promise": "3.3.0",
    "@types/execa": "0.9.0",
    "@types/fs-extra": "5.0.1",
    "@types/js-yaml": "3.11.1",
    "@types/lodash": "4.14.109",
    "@types/minimist": "1.2.0",
    "@types/mz": "0.0.32",
    "@types/recursive-readdir": "2.2.0",
    "chokidar": "2.0.4",
    "del": "3.0.0",
    "execa": "0.10.0",
    "fs-extra": "6.0.1",
    "js-yaml": "3.11.0",
    "lodash": "4.17.21",
    "mz": "2.7.0",
    "recursive-readdir": "2.2.2",
    "tslib": "1.9.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.13.34",
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.2",
    "@types/node": "10.3.1",
    "globby": "8.0.1",
    "ts-node": "6.1.0",
    "typescript": "2.9.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/crucialfelix/multigat",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "multigat",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "crucialfelix/multigat",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p . --noEmit",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "link": "npm link"
  },
  "types": "lib/index.d.ts"
}
